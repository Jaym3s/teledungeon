- form_for(call) do |f|
  = f.error_messages
  %p
    = f.label :caller
    %br
    = f.text_field :caller
  %p
    = f.label :called
    %br
    = f.text_field :called
  %p
    = f.label :callguid
    %br
    = f.text_field :callguid
  %p
    = f.label :channeltype
    %br
    = f.text_field :channeltype
  %p
    = f.label :callerinput
    %br
    = f.text_area :callerinput
  %p
    = f.label :callerinputhistory
    %br
    = f.text_area :callerinputhistory
  %p
    = f.label :callername
    %br
    = f.text_field :callername
  %p
    = f.label :callerlocation
    %br
    = f.text_field :callerlocation
  %p
    = f.submit "Submit"